import React from 'react'
import './Navbar.css'
import {Button} from 'react-bootstrap'

function NavBtn(props){
    return( 
        <div>
            {props.isLogin ? (
                <Button variant="primary">Account</Button>
            ):(
                <>
                <button>Login</button>
                <button>Register</button>
                </>
            )}
        </div>
        
    )
}

function Navbar(){

    return(
        <div className="navbar">
            <div className="logo">
                <span>FoodWays</span>
                <img src="" alt="logo" />
            </div>
            <NavBtn isLogin={true}/>
        </div>
    )
}

export default Navbar